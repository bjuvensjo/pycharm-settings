<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add">
        <Item>
          <IdeaActionID>Git.Add</IdeaActionID>
          <ShortCut>'⌥⌘A'</ShortCut>
          <Description>Add</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'⌘[' or '⌥⌘←' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>11</Count>
        </Item>
      </Statistic>
      <Statistic Action="Next Project Window">
        <Item>
          <IdeaActionID>NextProjectWindow</IdeaActionID>
          <ShortCut>'⌘`'</ShortCut>
          <Description>Next Project Window</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R' or 'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'test_create_repo'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R' or '⌃X, ⌃X'</ShortCut>
          <Description>Run 'test_create_repo'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run...">
        <Item>
          <IdeaActionID>ChooseRunConfiguration</IdeaActionID>
          <ShortCut>'⌃⌥R'</ShortCut>
          <Description>Run...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>ChangesView.Diff</IdeaActionID>
          <ShortCut>'⌘D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'continuous_delivery_csl'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'continuous_delivery_csl'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'filter_branch'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'filter_branch'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'test-all'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'test-all'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>